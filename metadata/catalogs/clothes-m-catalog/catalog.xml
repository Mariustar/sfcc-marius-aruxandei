<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns="http://www.demandware.com/xml/impex/catalog/2006-10-31" catalog-id="clothes-m-catalog">
    <header>
        <image-settings>
            <internal-location base-path="/"/>
            <view-types>
                <view-type>large</view-type>
                <view-type>medium</view-type>
                <view-type>small</view-type>
                <view-type>swatch</view-type>
            </view-types>
            <alt-pattern>${productname}</alt-pattern>
            <title-pattern>${productname}</title-pattern>
        </image-settings>
    </header>

    <category category-id="root">
        <display-name xml:lang="x-default">PagMan Catalog</display-name>
        <online-flag>true</online-flag>
        <template/>
        <page-attributes/>
        <custom-attributes>
            <custom-attribute attribute-id="enableCompare">false</custom-attribute>
            <custom-attribute attribute-id="showInMenu">true</custom-attribute>
        </custom-attributes>
    </category>

    <product product-id="DAD23">
        <ean/>
        <upc/>
        <unit/>
        <min-order-quantity>1</min-order-quantity>
        <step-quantity>1</step-quantity>
        <display-name xml:lang="de">Papucei</display-name>
        <display-name xml:lang="x-default">Papucei</display-name>
        <display-name xml:lang="fr">Papucei</display-name>
        <short-description xml:lang="de">Diese sind klassische Schuhe</short-description>
        <short-description xml:lang="x-default">These are classic shoes</short-description>
        <short-description xml:lang="fr">Ce sont les chaussures classiques</short-description>
        <store-force-price-flag>false</store-force-price-flag>
        <store-non-inventory-flag>false</store-non-inventory-flag>
        <store-non-revenue-flag>false</store-non-revenue-flag>
        <store-non-discountable-flag>false</store-non-discountable-flag>
        <online-flag>false</online-flag>
        <available-flag>false</available-flag>
        <searchable-flag>false</searchable-flag>
        <page-attributes/>
        <pinterest-enabled-flag>false</pinterest-enabled-flag>
        <facebook-enabled-flag>false</facebook-enabled-flag>
        <store-attributes>
            <force-price-flag>false</force-price-flag>
            <non-inventory-flag>false</non-inventory-flag>
            <non-revenue-flag>false</non-revenue-flag>
            <non-discountable-flag>false</non-discountable-flag>
        </store-attributes>
    </product>

    <product product-id="Marius">
        <ean/>
        <upc/>
        <unit/>
        <min-order-quantity>1</min-order-quantity>
        <step-quantity>1</step-quantity>
        <display-name xml:lang="x-default">Marius</display-name>
        <store-force-price-flag>false</store-force-price-flag>
        <store-non-inventory-flag>false</store-non-inventory-flag>
        <store-non-revenue-flag>false</store-non-revenue-flag>
        <store-non-discountable-flag>false</store-non-discountable-flag>
        <online-flag>true</online-flag>
        <available-flag>false</available-flag>
        <searchable-flag>true</searchable-flag>
        <page-attributes/>
        <variations>
            <attributes>
                <variation-attribute attribute-id="color" variation-attribute-id="color" slicing-attribute="true">
                    <display-name xml:lang="x-default">color</display-name>
                </variation-attribute>
            </attributes>
        </variations>
        <classification-category>root</classification-category>
        <pinterest-enabled-flag>false</pinterest-enabled-flag>
        <facebook-enabled-flag>false</facebook-enabled-flag>
        <store-attributes>
            <force-price-flag>false</force-price-flag>
            <non-inventory-flag>false</non-inventory-flag>
            <non-revenue-flag>false</non-revenue-flag>
            <non-discountable-flag>false</non-discountable-flag>
        </store-attributes>
    </product>

    <product product-id="set-1">
        <ean/>
        <upc/>
        <unit/>
        <min-order-quantity>1</min-order-quantity>
        <step-quantity>1</step-quantity>
        <store-force-price-flag>false</store-force-price-flag>
        <store-non-inventory-flag>false</store-non-inventory-flag>
        <store-non-revenue-flag>false</store-non-revenue-flag>
        <store-non-discountable-flag>false</store-non-discountable-flag>
        <online-flag>true</online-flag>
        <available-flag>false</available-flag>
        <searchable-flag>false</searchable-flag>
        <page-attributes/>
        <product-set-products>
            <product-set-product product-id="Marius"/>
        </product-set-products>
        <pinterest-enabled-flag>false</pinterest-enabled-flag>
        <facebook-enabled-flag>false</facebook-enabled-flag>
        <store-attributes>
            <force-price-flag>false</force-price-flag>
            <non-inventory-flag>false</non-inventory-flag>
            <non-revenue-flag>false</non-revenue-flag>
            <non-discountable-flag>false</non-discountable-flag>
        </store-attributes>
    </product>

    <category-assignment category-id="root" product-id="DAD23">
        <primary-flag>true</primary-flag>
    </category-assignment>

    <category-assignment category-id="root" product-id="Marius">
        <primary-flag>true</primary-flag>
    </category-assignment>

</catalog>
